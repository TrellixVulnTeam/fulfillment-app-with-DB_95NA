tton\"\r\n        class=\"btn btn-primary m-1 refresh\"\r\n        v-on:click=\"refreshData()\"\r\n      >\r\n        Refresh\r\n      </button>\r\n    </div>\r\n     <div class=\"orders-counr\">number of orders here :{{logs.length}}</div>\r\n    <table class=\"table table-light\">\r\n      <thead>\r\n        <tr>\r\n         \r\n          <th scope=\"col\">#ID</th>\r\n          <th scope=\"col\">Total Price</th>\r\n          <th scope=\"col\">Customer</th>\r\n          <th scope=\"col\">Destination</th>\r\n          <th scope=\"col\">created at</th>\r\n          <th scope=\"col\">fulfilled at</th>\r\n          <th scope=\"col\">Items SKU</th>\r\n          <th scope=\"col\">Description</th>\r\n          <th scope=\"col\">label</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <logscomponents\r\n          v-for=\"order in logs\"\r\n          :key=\"order.order_id\"\r\n          :id=\"order.order_id\"\r\n          :total_price=\"order.total_price\"\r\n          :customer=\"order.customer\"\r\n          :itemSku=\"order.items_sku\"\r\n          :country =\"order.country\"\r\n          :description=\"order.items_description\"\r\n          :created_at=\"order.created_at\"\r\n          :fulfilled_at=\"order.fulfilled_at\"\r\n          :label=\"order.label\"\r\n        />\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n<style>\r\n.loading {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: fixed;\r\n  background-color: #fff;\r\n  display: none;\r\n  top: 0px;\r\n  left: 0px;\r\n}\r\n.spinner-border.text-dark {\r\n  margin: 25% 50%;\r\n}\r\n</style>\r\n<script>\r\nconst endpoint = \"http://localhost:8081/\";\r\nimport logscomponents from \"../components/logscomponents.vue\";\r\n// @ is an alias to /src\r\nexport default {\r\n  data: function () {\r\n    return {\r\n      logs: [],\r\n      loaded:false\r\n    };\r\n  },   created(){\r\n      var requestOptions = {\r\n        method: \"GET\",\r\n        redirect: \"follow\",\r\n        Headers:{\r\n          \"mode\": \"no-cors\"\r\n        }\r\n      };\r\n      fetch(endpoint + \"getlogs\", requestOptions)\r\n      .then((response) =>{\r\n        console.log(response);\r\n      })\r\n      .then((result) => {\r\n            console.log(result);\r\n            setTimeout(()=>{\r\n            fetch(endpoint + \"logs.json\")\r\n         .then((res) => res.json())\r\n          .then((data) => {this.logs = data;\r\n          this.loaded = true})\r\n          .catch((err) => console.log(err.message));\r\n            },1500)\r\n          })\r\n  },\r\n  methods: {\r\n    refreshData: function () {\r\n      document.querySelector(\".loading\").style.display = \"block\";\r\n      \r\n\r\n      setTimeout(function () {\r\n        window.location.reload();\r\n        document.querySelector(\".loading\").style.display = \"none\";\r\n      }, 1500);\r\n    },\r\n    \r\n  },\r\n  components: { logscomponents },\r\n  name: \"logs\",\r\n};\r\n</script>\r\n"]}]}